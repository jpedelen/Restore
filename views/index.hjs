    <div class="container">

     <form class="form-horizontal" action="/save_form" method="POST">
<a href="/images/HEADERrestore.jpg"><img src="/images/HEADERrestore.jpg" alt=""></a>
<fieldset>

<!-- Form Name -->
<legend>ReStore: Receipt for in-kind donation</legend>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="first_name">Name of donor:</label>
  <div class="controls">
    <input id="first_name" name="first_name" type="text" placeholder="First name" class="input-xlarge" required="">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="last_name"></label>
  <div class="controls">
    <input id="last_name" name="last_name" type="text" placeholder="Last name" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="street_address">Address:</label>
  <div class="controls">
    <input id="street_address" name="street_address" type="text" placeholder="Street" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="state">State:</label>
  <div class="controls">
    <input id="state" name="state" type="text" placeholder="State" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="zip_code">ZIP:</label>
  <div class="controls">
    <input id="zip_code" name="zip_code" type="text" placeholder="5 digit zip code" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="phone">Phone:</label>
  <div class="controls">
    <input id="phone" name="phone" type="text" placeholder="(319) xxx-xxxx" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="email">Email:</label>
  <div class="controls">
    <input id="email" name="email" type="text" placeholder="name@xyz.com" class="input-xlarge">
    
  </div>
</div>

<!-- Multiple Checkboxes -->
<div class="control-group">
  <label class="control-label" for="volunteer_checkbox"></label>
  <div class="controls">
    <label class="checkbox" for="volunteer_checkbox-0">
      <input type="checkbox" name="volunteer_checkbox" id="volunteer_checkbox-0" value="Check here to receive more information about volunteer opportunities, financial support, and special events with CVHFH.">
      Check here to receive more information about volunteer opportunities, financial support, and special events with CVHFH.
    </label>
  </div>
</div>

<!-- Textarea -->
<div class="control-group">
  <label class="control-label" for="donated_items">Description of item(s) donated:</label>
  <div class="controls">                     
    <textarea id="donated_items" name="donated_items"></textarea>
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="agent">ReStore Agent</label>
  <div class="controls">
    <input id="agent" name="agent" type="text" placeholder="Agent name" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="date">Date:</label>
  <div class="controls">
    <input id="date" name="date" type="text" placeholder="xx/xx/xxxx" class="input-xlarge">
    
  </div>
</div>

<!-- Button -->
<div class="control-group">
  <label class="control-label" for="submit_button"></label>
  <div class="controls">
    <input type='submit' id="submit_button" name="submit_button" class="btn btn-primary" value="Submit">
  </div>
</div>
</fieldset>
</form>


    </div><!-- /.container -->


